    "using Turing\n",
    "\n",
    "# Declare Turing model\n",
    "@model AXLturing() = begin\n",
    "    p = Array{Real}(undef, 15)\n",
    "    p ~ [LogNormal(0.0, 0.5)]\n",
    "    p[3] ~ Beta(1.0, 1.0)\n",
    "\n",
    "    TAMode.runTAM([10.0], p, 100.0)\n",
    "end;"
   ]
